<!--Whole Master page div-->
<div id="masterPageWholeBody">
<!-- Screen Header HTML-->

    <!--Upper Top Default Blue Bar-->
    <div id="StartColorDiv" style="height:20px; width:100%; color:#0067BB; background-color:#0067BB;">
           Blue Color
    </div>


    <!--Middle Top Default Tag-->
	<div id="headerDiv" style="align-self:center">
        <input type="hidden" id="companyIdStoringTextField"/>
        <table style="width:99%">
            <tr>
                <td style="text-align:left;">
                    <h2>
				        <span style="color:#0067BB; font-weight:800;">WIN</span><span style="color:black; font-weight:800;">MONITOR</span>
			        </h2>
                </td>
                <td  style="text-align:right;">
                    <img src="@Url.Content("~/Content/Images/CustomerLogo.png")" style="width:200px; height:40px; margin-right:5px;"/><br />
                </td>
            </tr>
            <tr>
                <td style="text-align:left;">
                    <div>
                        Welcome, <span id="spanForAccount" style="color:#0067BB"></span>
                    </div>
                </td>
                <td style="text-align:right;">
                    <div id="companyAccountDiv">
                        Account: <span id="spanForCompanyAccountName" style="color:#0067BB"></span>
                    </div>
                </td>
            </tr>
            <tr>
                <td colspan="2" style="text-align:right;">
                    <button type="button"  class="btn btn-link" id="btnLogout" style="font-weight:500; font-size:18px; height:15px; margin:5px; color: #67747c;" onclick="SignOutFromApplication()">
						Logout
					</button>
                </td>
            </tr>
        </table>
        <br>
	</div>


    <!--Lower Top Default Black Bar-->
    <div id="EndColorDiv" style="height:25px; width:100%; color:#484848; background-color:#484848;">
           Black Color
    </div>

    <!--Screen Body-->
    <table style="width:100%">
        <tr>
            <td style="vertical-align:top;">
			    <!--master page tab button panel-->
                <div class="btn-group-vertical" style="width:200px;" id="vertical-button-group">
				    <button type="button" class="btn btn-link masterPageTabButton" id="btndashboard" onclick="masterPageDashBoardClicked(this)">
					    <b>Dashboard</b>
				    </button>
				    <button type="button" class="btn btn-link masterPageTabButton" id="btncomponents" onclick="masterPageComponentClicked(this)">
					    <b>Components</b>
				    </button>
				    <button type="button" class="btn btn-link masterPageTabButton" id="btnactivate" onclick="masterPageMaintenanceCalendarClicked(this)">
					    <b>Maintenance Calendar</b>
				    </button>
				    <button type="button" class="btn btn-link masterPageTabButton" id="btnregistered" onclick="masterPageRegisteredAdminClicked(this)">
					    <b>Administrators</b>
				    </button>
			    </div>
            </td>
            <!--page content placement place-->
		    <td style="width:100%; height:100%; vertical-align:top;" id="masterPageContentPlace">
		    </td>
        </tr>
    </table>

 <!--to add custom.js functions after body load-->
<script src="~/Scripts/custom.js"></script>
    <!--Screen Footer-->
    <div id="footer" class="row-fluid s4-notdlg">
	        <div class="container-fluid">
		        <div id="footer-links">
			        <ul>
