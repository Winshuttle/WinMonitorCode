@using WinMonitorApp.Models

<!-- Defining Tags for Using JQuery-->

<script src="~/Scripts/jquery-1.10.1.min.js"></script>
<script src="~/Scripts/jquery-1.9.1.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>
<script src="~/Scripts/bootstrap.js"></script>

<!--Bootstrap Value imports-->
<meta name="viewport" content="width=device-width, initial-scale=1">

<!--For IE compatibility-->
<meta http-equiv="X-UA-Compatible" content="IE=9; IE=8; IE=7" />
<link rel="stylesheet" href="~/css/bootstrap.min.css"/>
<link rel="stylesheet" href="~/css/bootstrap-theme.min.css"/>

<script src="~/Scripts/bootstrap.min.js"></script>

<script src="~/Scripts/customUser.js"></script>

<link rel="stylesheet" href="~/css/custom.css" />

<link rel="stylesheet" href="../../css/jquery.dataTables.min.css">

<script type="text/javascript" src="../../Scripts/jquery.dataTables.min.js"></script>

<script src="~/Scripts/moment.min.js"></script>

 <!--Upper Top Default Blue Bar-->
    <div id="StartUserColorDiv" style="height:20px; width:100%; color:#0067BB; background-color:#0067BB;">
           Blue Color
    </div>

<script>
    $(document).ready(function () {
        document.title = "WinMonitor";
    });

    //function to display popover plugin
    $(function () {
        $('#SubscribeUpdates').popover({

            placement: 'bottom',
            title: 'Subscription Form',
            html: true,
            content: $('#mySubscribeForm').html()
        });
    });

</script>

<!--Middle Top Default Div-->
<div id="userMasterPageMainDiv">
    <div id="body" style="width:99%; margin:5px;">
        <div id="header" style="width:100%; height:auto;">
            <div id="label" style="width:100%; float:left; font-size: 250%; margin:10px;">
                <span style="color:#0067BB; font-weight:800;">WIN</span><span style="color:black; font-weight:800">MONITOR</span>
                <img src="@Url.Content("~/Content/Images/CustomerLogo.png")" style="float:right; width:200px; height:40px; margin:10px;" />
            </div><br />

            <input type="hidden" id="UserCompanyIdReturn" value="@ViewBag.mUserCompanyId" />
            <h3 style="margin:10px; font-size:large;"><span>Account: </span><span style="color:#0067BB;">@ViewBag.mUserCompanyName</span></h3>


            <!--Down Black using Navbar-->
            <nav class="navbar-left" style="width:100%; background-color:#484848;">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar" style="background-color:#484848;">
                            <img src="@Url.Content("~/Content/Images/collapse-Button-Image.png")" />
                        </button>
                    </div>
                    <div class="collapse navbar-collapse" id="myNavbar" style="background-color:#484848;">
                        <ul class="nav nav-tabs navbar-left" style="font-size: 150%;" id="navbarLinks">
                            <li><a href="~/User/statusUserView/?commonCheck=CheckIfTrueErrorWin" target="content" id="frameStatusUserView" style="height:14px;font-size:medium; text-decoration:none;"><span style="color:white;">Status</span></a></li>
                            <li><a href="~/User/historyUserView/?commonCheck=CheckIfTrueErrorWin" target="content" id="frameHistoryUserView" style="height:14px; font-size:medium; text-decoration:none;"><span style="color:white;">History</span></a></li>
                            <li><a href="~/User/Performance/?commonCheck=CheckIfTrueErrorWin" target="content" id="framePerformanceUserView" style="height:14px; font-size:medium; text-decoration:none;"><span style="color:white;">Performance</span></a></li>
                            <li><a href="~/User/MaintenanceCalendar/?commonCheck=CheckIfTrueErrorWin" target="content" id="frameMaintenanceCalendar" style="height:14px; font-size:medium; text-decoration:none;"><span style="color:white;">Maintenance Calendar</span></a></li>
                        </ul>
                    </div>
                </div>
            </nav>
        </div>
        <iframe id="content" name="content" style="width:100%; height:75%; border: 1px solid #DEB887;" src="~/User/statusUserView/?commonCheck=CheckIfTrueErrorWin"></iframe>

        <div id="footer" class="row-fluid s4-notdlg">
            <div class="container-fluid">
                <div id="footer-links">
                    <ul>
                        <li style="list-style-type:none; display:inline;"><a href="https://help.winshuttle.com/foundation " target="_blank">Help</a>&nbsp;&nbsp;</li>
                        <li style="list-style-type:none; display:inline;"><a href="https://www.winshuttle.com/support-training/" target="_blank">Support</a>&nbsp;&nbsp;</li>
                        <li style="list-style-type:none; display:inline;"><a href="https://www.winshuttle.com/About" target="_blank">About</a>&nbsp;&nbsp;</li>
                        <li style="list-style-type:none; display:inline;"><a href="https://www.winshuttle.com/Privacy" target="_blank">Privacy</a>&nbsp;&nbsp;</li>
                        <li style="list-style-type:none; display:inline;"><a href="https://www.winshuttle.com/EULA" target="_blank">EULA</a>&nbsp;&nbsp;</li>
                        <li style="list-style-type:none; display:inline; float:right">© 2008-2016 Winshuttle, LLC</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>


<!--display incident details-->
   <div class = "modal fade" id = "eventClickEventModalInUser" tabindex = "-1" role = "dialog" aria-labelledby = "clickEventCalendarModalInUserLabel" aria-hidden = "true" >
                <div class = "modal-dialog">
                    <div class = "modal-content">
                        
                        <!--dialog box header-->
                        <div class = "modal-header">
                            
                             <!--dialog box close button-->
                            <button type = "button" class = "close" data-dismiss = "modal" aria-hidden = "true">
                                &times;
                            </button>
            
                            <!--dialog box label--> 
                            <h4 class = "modal-title" id = "clickEventCalendarModalInUserLabel" style="color:#0067BB;">
                                 <b>Event Details</b>
                             </h4>
                     </div>
         
                <!--dialog box body-->
                <div class = "modal-body">
                     <form style="font-size:medium; font-weight:600;">
                         <table style="width:100%;border-spacing: 20px;" >
                             <tr>
                                 <td style="width:33%; text-align:left;">
                                     <span>Title</span>
                                 </td>
                                 <td style="width:66%; text-align:left;">
                                     <input type="text" style="border:none; width:100%;" id="EventTitleShowInUser" readonly="readonly"/>
                                 </td>
                             </tr>
                             <tr>
                                 <td>
                                </td>
                            </tr>
                             <tr>
                                 <td style="width:33%; text-align:left;vertical-align:top">
                                     <span>Details</span>
                                 </td>
                                 <td style="width:66%; text-align:left;">
                                     <textarea rows="3" cols="22" style="border:none; width:100%;" id="EventDetailsShowInUser" readonly="readonly"></textarea>
                                 </td>
                             </tr>
                             <tr>
                                 <td>
                                </td>
                            </tr>
                             <tr>
                                 <td style="width:33%; text-align:left;">
                                     <span>Start Time</span>
                                 </td>
                                 <td style="width:66%; text-align:left;">
                                     <input type="text" style="border:none; width:100%;" id="datetimepickerStartTimeShowInUser" readonly="readonly"/>
                                 </td>
                             </tr>
                             <tr>
                                 <td>
                                </td>
                            </tr>
                             <tr>
                                 <td style="width:33%; text-align:left;">
                                     <span>End Time</span>
                                 </td>
                                 <td style="width:66%; text-align:left;">
                                     <input type="text" style="border:none; width:100%;" id="datetimepickerEndTimeShowInUser" readonly="readonly"/>
                                 </td>
                             </tr>
                             <tr>
                                 <td>
                                </td>
                            </tr>
                             <tr>
                                 <td style="width:33%; text-align:left;">
                                     <span>Type</span>
                                 </td>
                                 <td style="width:66%; text-align:left;">
                                     <input type="text" style="border:none; width:100%;" id="selectTypeMaintenanceShowInUser" readonly="readonly"/>
                                 </td>
                             </tr>
                             <tr>
                                 <td>
                                </td>
                            </tr>
                             <tr>
                                 <td style="width:33%; text-align:left;">
                                     <span>Status</span>
                                 </td>
                                 <td style="width:66%; text-align:left;">
                                     <input type="text" style="border:none; width:100%;" id="StatusShowInUser" readonly="readonly"/>
                                 </td>
                             </tr>
                         </table><br>
                     </form>
                 </div>
         
                <!--dialog box footer-->
                <div class = "modal-footer">
                        <!--dialog box Close Button-->
                        <button type = "button" class = "btn btn-default" data-dismiss = "modal">
                           Close
                        </button>
                </div>
            </div>                                                
        </div>                                               
    </div>